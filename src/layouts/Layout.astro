---
const buyList = [
  'Controller',
  'Bread',
  'Water',
  'Notebook',
  'Math Book',
  'Choco Bar',
  'Milk'
];

function ListaDeCompra() {
  return (
    <div>
      <h2>Lista de Compra</h2>
      <ul>
        {buyList.map((item, index) => (
          <li key={index}>{item}</li>
        ))}
      </ul>
    </div>
  );
}

// Uso del componente ListaDeCompra
function App() {
  return <ListaDeCompra />;
}
